{
  "name": "gatsby-background-image-lite",
  "version": "0.0.0-development",
  "description": "Lite-weight solution for setting assets from Gatsby's image pipeline as backgrounds",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keplersj/gatsby-background-image-lite.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-image",
    "css",
    "webp",
    "images"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keplersj/gatsby-background-image-lite/issues"
  },
  "homepage": "https://github.com/keplersj/gatsby-background-image-lite#readme",
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "polished": "^3.4.2",
    "react-intersection-observer": "^8.25.2",
    "react-use-webp-support-check": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@emotion/jest": "^11.0.0-next.10",
    "@testing-library/react": "^9.3.3",
    "@types/jest": "^24.9.0",
    "@types/react": "^16.9.16",
    "babel-plugin-emotion": "^10.0.27",
    "gatsby": "^2.18.11",
    "jest": "^25.1.0",
    "jest-preset-gatsby": "^1.0.21",
    "jest-runner-prettier": "^0.3.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "semantic-release": "^16.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "gatsby": "^2.0.0"
  }
}
